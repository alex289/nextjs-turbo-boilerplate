{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db-generate"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "dev": {
      "dependsOn": ["^db-generate"],
      "cache": false
    },
    "db-generate": {
      "cache": true
    },
    "db-push": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
